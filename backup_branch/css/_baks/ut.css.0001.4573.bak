 *				table														*
 ********************************************************************************/
.myTable{
	width:100%;
	text-align:left;
}
.myTable th:first-child{
	width:85%;
}
.myTable td:nth-child(2){
	text-align:center;
}
.myTable tr td img
{
	padding-right:8px;
}
.myTable tr th{
	background-color:#999999;
	background-image:none;
	font-size:19px;
	font-weight:bold;
}
tr.alt td{
	background-color:#CCC;
}

